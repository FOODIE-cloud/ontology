############### Administrative Unit #############

map:gadm36_2_Administrative_Unit
    rr:logicalTable [ rr:tableName "`gadm36_2`"; ];
    rr:subjectMap [ rr:class lcc-cr:GeographicRegionKind; rr:template "http://w3id.org/foodie/open/gadm36/country/administrativeUnit/{`ENGTYPE_2`}"; ];
		
		rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:datatype xsd:string;
            rr:template "{`ENGTYPE_2`}"; 
        ];
    ];
    
