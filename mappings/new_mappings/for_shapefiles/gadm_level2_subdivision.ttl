@prefix geof: <http://www.opengis.net/def/function/geosparql/>.
@prefix map: <#>.
@prefix geo: <http://www.opengis.net/ont/geosparql#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rrx: <http://www.w3.org/ns/r2rml-ext#>.
@prefix rrxf: <http://www.w3.org/ns/r2rml-ext/functions/def/>.
@prefix strdf: <http://strdf.di.uoa.gr/ontology#>.
@prefix vocab: <ontology#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix lcc-cr: <https://www.omg.org/spec/LCC/Countries/CountryRepresentation#> .
@prefix gn: <http://www.geonames.org/ontology#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/terms/>.
@prefix juso: <http://rdfs.co/juso#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .

################## Geometry ###################

map:gadm36_2_geometry
    rr:logicalTable [ rr:tableName "`gadm36_2`"; ];
    rr:subjectMap [ rr:class geo:Geometry; rr:template "http://w3id.org/foodie/open/gadm36/country/subdivision/level2/geometry/{`gid`}"; ];
    rr:predicateObjectMap [
        rr:predicate geo:isSimple;
        rr:objectMap [
            rr:datatype xsd:boolean;
            rrx:function rrxf:isSimple;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:coordinateDimension;
        rr:objectMap [
            rr:datatype xsd:integer;
            rrx:function rrxf:coordinateDimension;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:spatialDimension;
        rr:objectMap [
            rr:datatype xsd:integer;
            rrx:function rrxf:spatialDimension;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:asWKT;
        rr:objectMap [
            rr:datatype geo:wktLiteral;
            rrx:function rrxf:asWKT;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:dimension;
        rr:objectMap [
            rr:datatype xsd:integer;
            rrx:function rrxf:dimension;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:isEmpty;
        rr:objectMap [
            rr:datatype xsd:boolean;
            rrx:function rrxf:isEmpty;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate geo:is3D;
        rr:objectMap [
            rr:datatype xsd:boolean;
            rrx:function rrxf:is3D;
            rrx:argumentMap 
            (
                 [ rr:column "`the_geom`"; ]
            )
        ];
    ];
    .
############## Subdivision_level2 ########

    
map:gadm36_2_Subdivision_level2
    rr:logicalTable [ rr:tableName "`gadm36_2`"; ];
    rr:subjectMap [ rr:class gn:Feature, lcc-cr:CountrySubdivision; 
                    rr:template "http://w3id.org/foodie/open/gadm36/country/subdivision/level2/{`GID_2`}"; ];
    
    ### extra dynamic type####
    rr:predicateObjectMap [
        rr:predicate rdf:type; ##fixed
        rr:objectMap [
            rr:termType rr:IRI;
            rr:template "http://w3id.org/foodie/open/gadm36/country/administrativeUnit#{`ENGTYPE_2`}"; ##fixed
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate dc:subject; 
        rr:objectMap [
            rr:datatype xsd:integer;
            rr:constant 2;
        ];
    ];
    
    rr:predicateObjectMap [
        rr:predicate geo:hasGeometry;
        rr:objectMap [
            rr:termType rr:IRI;
            rr:template "http://w3id.org/foodie/open/gadm36/country/subdivision/level2/geometry/{`gid`}";
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate skos:notation;
        rr:objectMap [
            rr:datatype xsd:string;
            rr:column "`HASC_2`";
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:datatype xsd:string;
            rr:column "`NAME_2`";
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate gn:parentCountry;
        rr:objectMap [
            rr:termType rr:IRI;
            rr:template "http://w3id.org/foodie/open/gadm36/country/{`GID_0`}";
        ];
    ]; 
    rr:predicateObjectMap [
        rr:predicate gn:parentFeature;
        rr:objectMap [
            rr:termType rr:IRI;
            rr:template "http://w3id.org/foodie/open/gadm36/country/subdivision/level1/{`GID_1`}";
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate gn:parentADM1;
        rr:objectMap [
            rr:termType rr:IRI;
            rr:template "http://w3id.org/foodie/open/gadm36/country/subdivision/level1/{`GID_1`}";
        ];
    ];
      
    
